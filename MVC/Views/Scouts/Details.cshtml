@model ScoutModel
@*Generated from Custom Template.*@
@*Model namespace using directive should be added to _ViewImports.cshtml.*@

@{
    ViewData["Title"] = "Details";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>Scout</h4>
    <hr />
        <dl class="row">
            <dt class="col-2">
                @Html.DisplayNameFor(model => model.UserName)
            </dt>
            <dd class="col-10">
                @Html.DisplayFor(model => model.UserName)
            </dd>
        </dl>
    <dl class="row">
        <dt class="col-2">
            @Html.DisplayNameFor(model => model.ClubNames)
        </dt>
        <dd class="col-10">
            @Html.DisplayFor(model => model.ClubNames)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary mr-2"  asp-action="Edit" asp-route-id="@Model?.Id">Edit</a>
    <a class="btn btn-success mr-2" asp-action="Index" asp-route-id="@Model?.Id">See the Scout Reports Written By @Model.UserName</a>
    <a class="btn btn-secondary" asp-action="Index">Back to List</a>
</div>



